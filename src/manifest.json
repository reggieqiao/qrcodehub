{
    "manifest_version": 2,
    "name": "QRcodehub",
    "version": "1.0",
    "description": "Generate QR code from URL",
    "icons": {
      "48": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs"
    ]
  }
  