{
    "manifest_version": 3,
    "name": "QRcodehub",
    "version": "1.1",
    "description": "Generate QR code from URL",
    "icons": {
      "48": "icon.png"
    },
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab"
    ]
  }
  